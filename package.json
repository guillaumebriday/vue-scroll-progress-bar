{
  "name": "@guillaumebriday/vue-scroll-progress-bar",
  "version": "0.5.0",
  "description": "Vue.js plugin for page scroll progress bar",
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "main": "dist/vue-scroll-progress-bar.umd.js",
  "module": "dist/vue-scroll-progress-bar.es.js",
  "unpkg": "dist/vue-scroll-progress-bar.min.js",
  "scripts": {
    "build:browser": "rollup -c build/rollup.config.browser.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "build": "yarn build:browser && yarn build:es && yarn build:umd",
    "prepublishOnly": "yarn build",
    "lint": "eslint --ext .js --ignore-path .gitignore build/ src/ --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "babel-eslint": "10.0.3",
    "eslint": "6.6.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.0.0",
    "rollup": "1.26.5",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.2",
    "rollup-plugin-vue": "5.1.2",
    "vue-template-compiler": "2.6.10"
  },
  "homepage": "https://github.com/guillaumebriday/vue-scroll-progress-bar",
  "license": "MIT"
}
