{
  "name": "@guillaumebriday/vue-scroll-progress-bar",
  "version": "0.5.0",
  "description": "Vue.js plugin for page scroll progress bar",
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "main": "dist/vue-scroll-progress-bar.umd.js",
  "module": "dist/vue-scroll-progress-bar.es.js",
  "unpkg": "dist/vue-scroll-progress-bar.min.js",
  "scripts": {
    "build:browser": "rollup -c build/rollup.config.browser.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "build": "yarn build:browser && yarn build:es && yarn build:umd",
    "prepublishOnly": "yarn build",
    "lint": "eslint --ext .js --ignore-path .gitignore build/ src/"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.2",
    "rollup": "1.32.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-vue": "5.1.6",
    "vue-template-compiler": "2.6.11"
  },
  "homepage": "https://github.com/guillaumebriday/vue-scroll-progress-bar",
  "license": "MIT"
}
